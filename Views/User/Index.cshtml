                        @Html.DisplayFor(modelItem => item.BreedId)
        </td>
        <td>
                        @Html.DisplayFor(modelItem => item.Bio)
        </td>
        <td>
                        @Html.DisplayFor(modelItem => item.Size)
        </td>
        <td>
                        @Html.DisplayFor(modelItem => item.CityID)
        </td>
        <td>
 @Html.ActionLink("Edit", "Edit", "User", new { id = item.Id }) |
                    @Html.ActionLink("Detail", "Detail", "User", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", "User", new { id = item.Id })
        </td>
    </tr>
            }
    </tbody>
</table>
